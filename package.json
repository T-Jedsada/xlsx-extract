{
  "name": "xlsx-extract",
  "version": "0.0.21",
  "license": "MIT",
  "description": "super-simple async XLSX reader with low memory footprint",
  "homepage": "https://github.com/ffalt/xlsx-extract",
  "keywords": [
    "xlsx",
    "tsv",
    "extract",
    "excel",
    "spreadsheet"
  ],
  "scripts": {
    "clean": "shx rm -rf dist",
    "build": "npm run clean && npm run build:plain && npm run build:esm && npm run build:bundle",
    "build:plain": "tsc --outDir dist/lib",
    "build:esm": "tsc -m es6 --outDir dist/lib-esm",
    "build:bundle": "webpack --mode production",
    "cover": "istanbul cover _mocha",
    "pretest": "tsc --outDir dist/lib",
    "lint": "tslint --project tsconfig.json \"src/**/*.ts\"",
    "test": "nyc mocha",
    "watch": "mocha-typescript-watch"
  },
  "dependencies": {
    "commander": "^2.15.1",
    "node-expat": "^2.3.16",
    "sax": "^1.2.4",
    "unzip2": "^0.2.5",
    "webpack-node-externals": "^1.7.2"
  },
  "devDependencies": {
    "@types/chai": "^4.1.4",
    "@types/debug": "0.0.30",
    "@types/mocha": "^5.2.2",
    "@types/node": "^10.3.6",
    "@types/sax": "^1.0.1",
    "@types/tmp": "0.0.33",
    "chai": "^4.1.2",
    "chai-exclude": "^1.0.8",
    "awesome-typescript-loader": "^5.2.0",
    "istanbul": "^0.4.5",
    "mocha": "^5.2.0",
    "mocha-typescript": "^1.1.16",
    "nyc": "^12.0.2",
    "shx": "^0.3.1",
    "source-map-support": "^0.5.6",
    "tmp": "0.0.33",
    "ts-node": "^6.1.1",
    "tslint": "^5.10.0",
    "typescript": "^2.9.2",
    "webpack": "^4.14.0",
    "webpack-cli": "^3.0.8"
  },
  "bugs": {
    "url": "https://github.com/ffalt/xlsx-extract/issues"
  },
  "author": {
    "name": "ffalt"
  },
  "contributors": [
    {
      "name": "Michael Pelikan",
      "url": "https://github.com/mpelikan"
    },
    {
      "name": "Mikuso",
      "url": "https://github.com/mikuso"
    },
    {
      "name": "Vasil Velichkov",
      "url": "https://github.com/velichkov"
    },
    {
      "name": "Laurence Hudson",
      "url": "https://github.com/laurence-hudson-tessella"
    },
    {
      "name": "Chrisdf",
      "url": "https://github.com/Chrisdf"
    }
  ],
  "bin": "./bin/xlsxe.js",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/ffalt/xlsx-extract"
  },
  "engines": {
    "node": ">= 4.0.0"
  },
  "nyc": {
    "temp-directory": "./coverage/.nyc_output",
    "report-dir": "./coverage",
    "include": [
      "src/**/*.ts"
    ],
    "exclude": [
      "test/**/*.ts"
    ],
    "require": [
      "ts-node/register",
      "source-map-support/register"
    ],
    "reporter": [
      "lcov",
      "text-summary",
      "html"
    ],
    "all": true,
    "check-coverage": true,
    "sourceMap": true,
    "instrument": true
  },
  "readmeFilename": "README.md"
}
